{
  "command": {
    "publish": {
      "allowBranch": "master",
      "conventionalCommits": true,
      "message": "chore(release): publish %s"
    }
  },
  "packages": [
    "packages/*"
  ],
  "ignoreChanges": [
    "**/__tests__/**",
    "**/*.md"
  ],
  "version": "2.2.0",
  "npmClient": "yarn",
  "registry": "https://registry.npmjs.org/"
}
